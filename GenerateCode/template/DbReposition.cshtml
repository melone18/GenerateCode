@("using System;")
@("using System.Collections.Generic;")
@("using System.Linq;")
@("using System.Text;")
@("using System.Threading.Tasks; ")



@("namespace ")@Model.NameSpace
@("{")
    @("public class ")@Model.Tablename@("PO")
